id: intel-tdx
unit: test plan
_name: Intel Trusted Domain eXtension (TDX) full tests
include:
    tdx-host/.*
    tdx-guest/.*
    tdx-boot/.*
    tdx-perf/.*
    tdx-quote/.*
    tdx-stress/.*
mandatory_include:
    com.canonical.certification::miscellanea/submission-resources
bootstrap_include:
    com.canonical.certification::executable
    com.canonical.certification::snap

